<div id="team">
	<div class="container text-center padded">
		<h2>Our team</h2>

		<% members = %w(seb anne romain) %>

		<ul class="list-inline">
			
			<% members.each do |member| %>
				<li>
					<%= image_tag "#{member}.jpg", class: "avatar-large" %>
					
					<h3><%= member %></h3>
				</li>
				
			<% end %>
		</ul>
	</div>
</div>